algoritmo "Básico"
//Anotações básicas em cada arquivo da aula

var
nome, sobrenome, nomeCompleto : literal
inicio

escreval("Informe o seu nome: ")
leia(nome)
escreval("Informe o seu sobrenome: ")
leia(sobrenome)
nomeCompleto <- nome + " " + sobrenome
escreval("Nome completo: " + nomeCompleto)

fimalgoritmo
