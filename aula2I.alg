algoritmo "Escolha"

var
voto: literal
inicio

escreval("Digite o seu voto")
leia(voto)

escolha voto
caso "20"
     escreval("Você votou no Marcelo)
caso "30"
     escreval("Você votou no Pernalonga")
caso "40"
     escreval("Você votou no Patolino")
outrocaso
         escreval("Você votou nulo")
fimescolha

fimalgoritmo
