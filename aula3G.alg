algoritmo "Comando Para Passo"

var
i, w, inicial, final : inteiro
inicio
escreval("Informe o valor inicial")
leia(inicial)
escreval("Informe o valor final")
leia(final)
escreval("Informe o valor do decremento")
leia(w)

para i de inicial ate final passo w faca
     escreval(i)
fimpara

fimalgoritmo
